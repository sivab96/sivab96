{
	"name": "Copy_Excel",
	"properties": {
		"activities": [
			{
				"name": "Copy Excel data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFolderPath": {
								"value": "@pipeline().parameters.test1",
								"type": "Expression"
							},
							"wildcardFileName": {
								"value": "@pipeline().parameters.Test2",
								"type": "Expression"
							},
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Test_Excel",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Emp_CSV",
						"type": "DatasetReference",
						"parameters": {
							"Directory": "target",
							"File_Name_CSV": "Test_CSV"
						}
					}
				]
			}
		],
		"parameters": {
			"test1": {
				"type": "String",
				"defaultValue": "Source"
			},
			"Test2": {
				"type": "String",
				"defaultValue": ".xlsx"
			}
		},
		"annotations": []
	}
}