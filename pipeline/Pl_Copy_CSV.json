							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Emp_CSV",
						"type": "DatasetReference",
						"parameters": {
							"Directory": "Source"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "Emp_CSV",
						"type": "DatasetReference",
						"parameters": {
							"Directory": "Target"
						}
